    <script>
        function Activate(link) {

            $("#navbar a").removeClass("active");


            $(link).addClass("active");
        }

        $(document).ready(function () {

            const currentPageLink = $(`#navbar a[href="${window.location.pathname}"]`);


            Activate(currentPageLink);
        });
    </script>
